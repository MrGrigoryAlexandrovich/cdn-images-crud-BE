{
  "name": "cdn-images-crud-be",
  "version": "1.0.0",
  "description": "This project is a REST API for managing images in a CDN (upload, fetch, delete) with metadata stored in MongoDB. The API can be integrated with frontends or other services that need image access.",
  "main": "index.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js"
  },
  "keywords": [
    "Node.js",
    "CDN",
    "cloudinary"
  ],
  "author": "Ahmed Cvrcak",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.6.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "cloudinary": "^2.7.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "mongoose": "^8.19.0",
    "multer": "^2.0.2"
  }
}
